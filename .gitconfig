[user]
  name = Johan Hanses
  email = johanhanses@gmail.com
[init]
  defaultBranch = main
[pull]
  rebase = false
[push]
  autoSetupRemote = true
  default = current
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[include]
  path = ~/themes.gitconfig
[delta]
  line-numbers = true
  navigate = true
  syntax-theme = OneHalfDark
  light = false
  side-by-side = false
  decorations = true
[core]
  excludesfile = ~/.gitignore_global
  pager = delta
[interactive]
  diffFilter = delta --color-only
[pager]
  diff = delta
  log = delta
  reflog = delta
  show = delta
[credential]
	helper = store
